#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int a[2];
    float b[2];

    scanf("%d %d", &a[0], &a[1]);
    scanf("%f %f", &b[0], &b[1]);
    printf("%d %d",a[0]+a[1], a[0]-a[1]);
    printf("\n%.1f %.1f",(b[0]+b[1]), (b[0]-b[1]));
    
    return 0;
}
